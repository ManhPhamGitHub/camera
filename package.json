{
  "name": "cloudops-authentication-partner",
  "version": "0.0.1",
  "description": "",
  "author": "osbkca",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "gen": "ts-node src/generator/generate-typings.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "yarn typeorm -d ./src/configs/typeorm/config.ts migration:run",
    "typeorm:generate-migration": "yarn typeorm -d ./src/configs/typeorm/config.ts migration:generate",
    "typeorm:create-migration": "yarn typeorm migration:create",
    "typeorm:revert-migration": "yarn typeorm migration:revert -d ./src/configs/typeorm/config.ts",
    "seed-cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js -r ./src/configs/typeorm -n seed-config.ts",
    "seed:config": "yarn seed-cli config",
    "seed:run": "yarn seed-cli seed",
    "documentation:serve": "npx @compodoc/compodoc -p tsconfig.json --serve"
  },
  "dependencies": {
    "@casl/ability": "^6.3.3",
    "@compodoc/compodoc": "^1.1.19",
    "@golevelup/nestjs-discovery": "^3.0.0",
    "@google-cloud/storage": "^7.11.1",
    "@grpc/grpc-js": "^1.8.11",
    "@grpc/proto-loader": "^0.7.5",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^0.0.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/microservices": "8.0.0",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/amqplib": "^0.8.2",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.6.0",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^2.2.0",
    "compression": "^1.7.4",
    "convict": "^6.2.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^10.0.0",
    "express-rate-limit": "^6.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "handlebars": "^4.7.7",
    "jwt-decode": "^3.1.2",
    "keycloak-connect": "^21.0.0",
    "moment": "^2.29.4",
    "mysql2": "^3.1.2",
    "nodemailer": "^6.9.13",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "serve-static": "^1.15.0",
    "slug": "^8.2.2",
    "typeorm": "^0.3.12",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chalk": "^2.2.0",
    "@types/convict": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
